{
  "manifest_version": 3,
  "name": "Sandboxed URL Opener",
  "version": "1.0",
  "description": "Open URLs in a sandboxed environment",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "sandbox": "sandbox allow-scripts; script-src 'self'"
  },
  "host_permissions": ["<all_urls>"]
}